{
  "name": "cooking-aide",
  "version": "1.0.0",
  "description": "A real-time vision application using the Overshoot SDK to read visible text from camera input",
  "type": "module",
  "main": "vision.js",
  "scripts": {
    "start": "node vision.js",
    "dev": "node --watch vision.js",
    "build": "tsc",
    "voice:build": "tsc",
    "voice:dev": "tsc --watch & node --watch dist/voice-agent.js dev",
    "voice:start": "npm run voice:build && node dist/voice-agent.js dev",
    "download-files": "npm run voice:build && node dist/voice-agent.js download-files",
    "token-server": "node token-server.js"
  },
  "keywords": [
    "vision",
    "overshoot",
    "text-recognition",
    "camera",
    "cooking",
    "realtime"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@livekit/agents": "^1.0.0",
    "@livekit/agents-plugin-livekit": "^1.0.0",
    "@livekit/agents-plugin-openai": "^1.0.0",
    "@livekit/agents-plugin-silero": "^1.0.0",
    "dotenv": "^16.0.0",
    "express": "^4.18.0",
    "livekit-server-sdk": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/cooking-aide.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/cooking-aide/issues"
  },
  "homepage": "https://github.com/yourusername/cooking-aide#readme"
}
